    "babel-preset-es2015": "^6.24.1",
    "bech32": "^2.0.0",
    "bs58check": "^4.0.0",
    "elliptic": "^6.6.1",
    "fake-indexeddb": "^6.0.0",
    "js-sha3": "^0.9.3",
